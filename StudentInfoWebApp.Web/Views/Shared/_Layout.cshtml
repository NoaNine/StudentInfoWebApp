<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"] - StudentInfoWebApp.Web</title>
    @await Html.PartialAsync("_CssPartial")
    @await Html.PartialAsync("_MetaTagsPartial")
</head>
<body>
    @await Html.PartialAsync("_HeaderPartial")
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    @await Html.PartialAsync("_FooterPartial")
    @await Html.PartialAsync("_ScriptsPartial")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
